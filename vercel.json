{
  "buildCommand": "echo 'Using pre-built output'",
  "installCommand": "npm install --ignore-scripts",
  "outputDirectory": "dist",
  "framework": null,
  "functions": {
    "api/*.js": {
      "runtime": "@vercel/node@3"
    }
  },
  "rewrites": [
    { "source": "/api/(.*)", "destination": "/api/$1" },
    { "source": "/((?!api/).*)", "destination": "/index.html" }
  ]
}
